  <process id="multiflow" name="multiflow" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="usertask1" name="提交申请" activiti:assignee="#{apply}">
      <extensionElements>
        <activiti:taskListener event="create" class="com.sekorm.base.workflow.listener.MyTaskCreateListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask2" name="主管审批" activiti:assignee="#{tlapprove}">
