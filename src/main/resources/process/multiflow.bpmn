        <activiti:taskListener event="create" class="com.sekorm.base.workflow.listener.MyTaskCreateListener"></activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="assigneeList" activiti:elementVariable="tlapprove">
        <completionCondition>${nrOfCompletedInstances/nrOfInstances &gt;= 0.9 }</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
